    "vite-node": "^0.26.2",
    "vitest": "^0.26.2",
    "vue": "^3.2.45",
    "vue-loader": "^17.0.1",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "vue-router": "4"
  }
}
